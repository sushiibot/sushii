{
  "compilerOptions": {
    "types": ["bun-types"],
    "target": "es2016",
    "sourceMap": true,
    "inlineSources": true,
    "sourceRoot": "/",
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "outDir": "./dist",
    "jsx": "react-jsx"
  },
  "include": ["./src/**/*.ts", "src/interactions/user/rank.service.tsx"]
}
