{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "lib": ["esnext"],
    "sourceMap": true,
    "inlineSources": true,
    "sourceRoot": "/",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "noEmit": true,
  },
  "include": ["./src/**/*.ts"],
}
